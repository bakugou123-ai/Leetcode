    def romanToInt(self, s: str) -> int:
        maps= {"I":1,"V":5,"X":10,"L":50,"C":100,"D":500,"M":1000}

        EXAMPLE : IV

        i=0
        summ=0
        n=len(s)

        while i<n:
            if i<n-1 and maps[s[i]]<maps[s[i+1]]:
                summ+= maps[s[i+1]] - maps[s[i]]
                i+=2

            else:
                summ+=maps[s[i]]
                i+=1
        return summ
